{
    "less": {
        "compress": true,
        "sourceMap": false,
        "plugins": ["autoprefix", "clean-css"],
        "globalVars": {
            "env": "production"
        }
    },
    "browserify": {
        "transforms": ["babelify", "uglifyify"],
