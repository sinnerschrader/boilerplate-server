{
  "name": "boilerplate-server",
  "version": "0.10.0",
  "description": "Lean and mean server module intended to get you kicking on the edge of things real fast.",
  "main": "library/",
  "scripts": {
    "start": "node binary/boilerplate-server.js",
    "prestart": "npm run build",
    "debug": "node-debug binary/boilerplate-server.js",
    "console": "node binary/boilerplate-console.js",
    "debug-console": "node-debug binary/boilerplate-console.js",
    "build": "npm run babel",
    "watch": "npm run babel -- --watch",
    "babel": "npm run clean && babel source --out-dir ./ --source-maps inline",
    "clean": "rm -rf application binary configuration library",
    "pre-publish": "npm run build && npm publish"
  },
  "bin": {
    "boilerplate-server": "binary/boilerplate-server.js",
    "boilerplate-console": "binary/boilerplate-console.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sinnerschrader/boilerplate-server.git"
  },
  "keywords": [
    "koa",
    "babel",
    "server"
  ],
  "author": {
    "name": "Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  },
  "dependencies": {
    "app-root-path": "^1.0.0",
    "atob": "^1.1.2",
    "babel-core": "^5.2.17",
    "bluebird": "^2.9.24",
    "commander": "^2.8.0",
    "find-root": "^0.1.1",
    "inquirer": "^0.8.5",
    "koa": "^0.19.1",
    "koa-basic-auth": "^1.1.2",
    "koa-conditional-get": "^1.0.3",
    "koa-etag": "^2.0.0",
    "koa-router": "^4.3.2",
    "koa-send": "^1.3.1",
    "lodash": "^3.7.0",
    "lodash.merge": "^3.3.2",
    "minimist": "^1.1.1",
    "portscanner": "^1.0.0",
    "rc": "^1.0.1",
    "require-all": "^1.0.0",
    "require-json": "0.0.1",
    "resolve-path": "^1.2.2",
    "winston": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^5.2.17",
    "babel-eslint": "^3.1.30",
    "eslint": "^1.10.3",
    "eslint-config-xo": "^0.8.0",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "node-inspector": "^0.12.1"
  }
}
